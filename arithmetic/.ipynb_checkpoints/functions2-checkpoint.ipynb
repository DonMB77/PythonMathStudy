    "from IPython.display import Math, display\n",
    "\n",
    "def computePower():\n",
    "    x = int(input(\"Please enter a base.\"))\n",
    "    y = int(input(\"Please enter an exponent.\"))\n",
    "    result = x**y\n",
    "    display(Math(\"%g^{%g} = %g\" %(x,y,result)))"
   ]
